{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "bknd",
  "main": "index.ts",
  "compatibility_date": "2025-11-25",
  "compatibility_flags": ["nodejs_compat"],
  "workers_dev": true,
  "minify": true,
  "assets": {
    "directory": "node_modules/bknd/dist/static",
  },
  "observability": {
    "enabled": true,
  },
  "d1_databases": [
    {
      "binding": "DB",
    },
  ],
  "r2_buckets": [
    {
      "binding": "BUCKET",
    },
  ],
}
